{
  "name": "smartlytics-api",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/atmus-tecnologia/smartlytics-api"
  },
  "description": "",
  "license": "UNLICENSED",
  "author": "Atmus Tecnologia <contato@atmustecnologia.com.br>",
  "contributors": [
    "Felipe Medeiros <medeiros.dev@gmail.com>"
  ],
  "scripts": {
    "build": "nest build",
    "compodoc": "compodoc -p tsconfig.json -s",
    "eslint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "eslint:fix": "npm run eslint:check -- --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "npm run eslint:check && npm run prettier:check",
    "precommit": "lint-staged && npm run lint && npm run test && npm run build",
    "prepare": "husky install",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,ejs,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,ejs,*rc}",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1",
      "^~/libs/storage(|/.*)$": "<rootDir>/libs/storage/src/$1"
    },
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ]
  },
  "dependencies": {
    "@bull-board/api": "^5.0.0",
    "@bull-board/express": "^5.0.0",
    "@bull-board/nestjs": "^5.0.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mapped-types": "^2.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "aws-sdk": "^2.1348.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "figlet": "^1.5.2",
    "gradient-string": "^2.0.2",
    "nestjs-paginate": "^8.1.7",
    "nodemailer": "^6.9.1",
    "openai": "^4.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/figlet": "^1.5.5",
    "@types/gradient-string": "^1.1.2",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
